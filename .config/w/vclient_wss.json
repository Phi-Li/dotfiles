{
  "log": {
    "access": "/var/log/v2ray/access_wss.log",
    "error": "/var/log/v2ray/error_wss.log",
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "port": 1090,
      "listen": "127.0.0.1",
      "protocol": "http",
      "settings": {
        "accounts": [],
        "allowTransparent": false,
        "userLevel": 0
      }
    }
  ],
  "outbounds": [
    {
      "sendThrough": "0.0.0.0",
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "",
            "port": 37192,
            "users": [
              {
                "id": "",
                "alterId": 4,
                "level": 0,
                "security": "auto"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "wsSettings": {
          "path": "/",
          "headers": {
            "Host": "v2ray.com"
          }
        }
      }
    },
    {
      "protocol": "freedom",
      "settings": {},
      "tag": "direct"
    },
    {
      "protocol": "blackhole",
      "settings": {},
      "tag": "blocked"
    }
  ],
  "routing": {
    "rules": [
      {
        "type": "field",
        "ip": [
          "geoip:private"
        ],
        "outboundTag": "blocked"
      }
    ]
  },
  "policy": {
    "levels": {
      "0": {
        "uplinkOnly": 0,
        "downlinkOnly": 0
      }
    }
  }
}